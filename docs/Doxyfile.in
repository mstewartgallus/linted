DOXYFILE_ENCODING = UTF-8

PROJECT_NAME = "@PACKAGE_NAME@"
PROJECT_NUMBER = "@PACKAGE_VERSION@"
PROJECT_BRIEF = "Linted uses practical protections against software bugs"

JAVADOC_AUTOBRIEF = YES

OPTIMIZE_OUTPUT_FOR_C = YES

INLINE_GROUPED_CLASSES = YES
INLINE_SIMPLE_STRUCTS  = YES

EXTRACT_ALL = YES
EXTRACT_LOCAL_CLASSES = NO

QUIET = YES

WARNINGS = YES

WARN_IF_UNDOCUMENTED = NO

RECURSIVE = YES
INPUT =\
  "@srcdir@/include"\
  "@srcdir@/src"\
  "@srcdir@/docs"\
  "@srcdir@/m4"\
  "@srcdir@/scripts"
STRIP_FROM_PATH="@srcdir@"

EXCLUDE_SYMBOLS = *_

SOURCE_BROWSER = YES

DISABLE_INDEX = YES

GENERATE_TREEVIEW = YES

ALIASES += error="\par Error:\n"

TAB_SIZE = 8

MAX_INITIALIZER_LINES = 0

GENERATE_HTML=YES
GENERATE_LATEX=NO

HTML_TIMESTAMP=NO
